{
  "name": "jr-gopnik",
  "version": "0.4.0",
  "description": "A multi-functional Discord bot for the modern users",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "node dist/index.js",
    "setup": "node dist/setup.js",
    "dev": "nodemon dist/index.js",
    "postinstall": "npm run build",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint src --ext .ts",
    "fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "discord",
    "bot",
    "akairo"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "GamesProSeif",
  "license": "MIT",
  "dependencies": {
    "color-convert": "^2.0.0",
    "common-tags": "^1.8.0",
    "discord-akairo": "github:discord-akairo/discord-akairo",
    "discord.js": "^12.0.1",
    "dotenv": "^7.0.0",
    "gamesproseif-common": "gamesproseif/gamesproseif-common",
    "inquirer": "^6.5.0",
    "mathjs": "^6.0.3",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^5.6.11",
    "node-emoji": "^1.10.0",
    "node-fetch": "^2.3.0",
    "node-opus": "^0.3.2",
    "punycode": "^2.1.1",
    "shorturl": "0.0.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.1",
    "ws": "^7.1.2",
    "youtube-search": "^1.1.4"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/color-convert": "^1.9.0",
    "@types/common-tags": "^1.8.0",
    "@types/dotenv": "^6.1.1",
    "@types/inquirer": "^6.5.0",
    "@types/moment-duration-format": "^2.2.2",
    "@types/mongoose": "^5.5.13",
    "@types/node": "^12.7.2",
    "@types/node-emoji": "^1.8.1",
    "@types/node-fetch": "^2.5.0",
    "@types/validator": "^10.11.3",
    "@types/ws": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "eslint": "^6.1.0",
    "eslint-config-marine": "^4.2.1",
    "nodemon": "^1.18.10",
    "typescript": "^3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GamesProSeif/jr-gopnik.git"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "zlib-sync": "^0.1.5"
  },
  "eslintConfig": {
    "extends": "marine/node",
    "rules": {
      "@typescript-eslint/explicit-member-accessibility": "off"
    }
  }
}
